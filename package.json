{
  "name": "yarn-tool",
  "version": "1.0.144",
  "description": "yarn cli tool helper",
  "keywords": [
    "cli",
    "data deduplication",
    "dedupe",
    "deduplicate",
    "deduplication",
    "diff",
    "duplicated",
    "install",
    "lerna",
    "lockfile",
    "npm",
    "package",
    "package manager",
    "packages",
    "project",
    "tool",
    "workspace",
    "workspaces",
    "ws",
    "yarn",
    "yarn-tool",
    "yarn.lock"
  ],
  "homepage": "https://github.com/bluelovers/yarn-tool#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/yarn-tool/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluelovers/yarn-tool.git"
  },
  "license": "ISC",
  "author": "bluelovers",
  "main": "index.js",
  "bin": {
    "yarn-tool": "bin/yarn-tool.js",
    "yt": "bin/yt.js"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "npm:publish": "yarn run version:bump && npm publish",
    "npm:publish:lerna": "npx lerna publish --yes --bump patch",
    "version:bump": "ts-node ./bin/yarn-tool version",
    "prepublish:yarn:install": "ts-node ./bin/yarn-tool install",
    "prepublishOnly": "yarn run ncu && yarn run prepublish:yarn:install && yarn run sort-package-json",
    "prepublishOnly_": "yarn run prepublishOnly:update && yarn run prepublishOnly:check-bin && yarn run test",
    "prepublishOnly:check-bin": "ynpx --quiet @yarn-tool/check-pkg-bin",
    "prepublishOnly:lockfile": "ynpx --quiet sync-lockfile",
    "prepublishOnly:update": "yarn run ncu && yarn run sort-package-json",
    "postpublish": "yarn run postpublish:changelog && yarn run postpublish:git:commit && yarn run postpublish:git:tag && yarn run postpublish:git:push",
    "postpublish:changelog": "ynpx --quiet @yarn-tool/changelog && git add ./CHANGELOG.md",
    "postpublish:git:commit": "git commit -m \"chore(release): publish\" . & echo postpublish:git:commit",
    "postpublish:git:push": "git push --follow-tags",
    "postpublish:git:tag": "ynpx --quiet @yarn-tool/tag",
    "help": "ts-node ./bin/yarn-tool --help",
    "ncu": "ts-node ./bin/yarn-tool ncu -u",
    "sort-package-json": "ts-node ./bin/yarn-tool sort"
  },
  "dependencies": {
    "@ts-type/package-dts": "^1.0.45",
    "@yarn-tool/find-root": "^1.0.36",
    "@yarn-tool/ncu": "^1.0.53",
    "@yarn-tool/npm-package-arg-util": "^1.0.8",
    "@yarn-tool/pkg-deps-util": "^1.0.9",
    "@yarn-tool/semver-diff": "^1.0.13",
    "@yarn-tool/static-file": "^1.0.52",
    "@yarn-tool/table": "^1.0.21",
    "@yarn-tool/update-notifier": "^1.0.15",
    "@yarn-tool/version-recommended": "^1.0.10",
    "@yarn-tool/yarnlock": "^1.0.41",
    "@yarn-tool/yarnlock-ncu": "^1.0.7",
    "array-hyper-unique": "^1.1.9",
    "bluebird": "^3.7.2",
    "create-yarn-workspaces": "^1.0.91",
    "crlf-normalize": "^1.0.5",
    "cross-spawn-extra": "^2.1.0",
    "debug-color2": "^1.2.3",
    "find-yarn-workspace-root2": "^1.2.23",
    "fs-extra": "^9.0.1",
    "inquirer": "^7.3.3",
    "npm-init2": "^1.0.145",
    "os-locale": "^5.0.0",
    "package-json": "^6.5.0",
    "semver": "^7.3.2",
    "semver-utils": "^1.1.4",
    "sort-package-json": "^1.44.0",
    "sort-package-json3": "^1.0.23",
    "source-map-support": "^0.5.19",
    "string-natural-compare": "^3.0.1",
    "synp2": "^1.4.0",
    "terminal-link": "^2.1.1",
    "upath2": "^3.1.6",
    "ws-root-spawn": "^1.0.15",
    "yargs": "^15.4.1",
    "yargs-parser": "^19.0.4",
    "yargs-unparser": "^1.6.3"
  },
  "devDependencies": {
    "@bluelovers/tsconfig": "^1.0.20",
    "@types/bluebird": "^3.5.32",
    "@types/deep-diff": "^1.0.0",
    "@types/fs-extra": "*",
    "@types/inquirer": "^7.3.1",
    "@types/lodash": "^4.14.161",
    "@types/node": "^14.6.4",
    "@types/package-json": "^5.0.1",
    "@types/semver": "^7.3.3",
    "@types/semver-utils": "^1.1.0",
    "@types/source-map-support": "^0.5.3",
    "@types/yargs": "^15.0.5",
    "@types/yargs-parser": "^15.0.0",
    "ts-type": "^1.2.34"
  },
  "peerDependencies": {
    "lerna": "^3.22.1",
    "yarn": "^1.22.5"
  }
}
