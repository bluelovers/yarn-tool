    "find-yarn-workspace-root2": "^1.2.12",
    "fs-extra": "^9.0.0",
    "is-npx": "^1.0.0",
    "npm-check-updates": "^5.0.0",
    "npm-init2": "^1.0.77",
    "os-locale": "^5.0.0",
    "package-json": "^6.5.0",
