{
  "name": "yarn-tool",
  "version": "1.0.82",
  "description": "yarn cli tool helper",
  "keywords": [
    "cli",
    "data deduplication",
    "dedupe",
    "deduplicate",
    "deduplication",
    "diff",
    "duplicated",
    "install",
    "lerna",
    "lockfile",
    "npm",
    "package",
    "package manager",
    "packages",
    "project",
    "tool",
    "workspace",
    "workspaces",
    "ws",
    "yarn",
    "yarn-tool",
    "yarn.lock"
  ],
  "homepage": "https://github.com/bluelovers/yarn-tool#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/yarn-tool/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluelovers/yarn-tool.git"
  },
  "license": "ISC",
  "author": "bluelovers",
  "main": "index.js",
  "bin": {
    "yarn-tool": "bin/yarn-tool.js",
    "yt": "bin/yt.js"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "npm:publish": "npm publish",
    "prepublish:lockfile": "npx sync-lockfile . & yarn run prepublish:lockfile:to:npm",
    "prepublish:lockfile:to:npm": "npx yarn-tool lockfile --npm --overwrite",
    "prepublish:yarn:install": "yarn-tool install",
    "prepublishOnly": "yarn run ncu && yarn run prepublish:yarn:install && yarn run prepublish:lockfile && npm run sort-package-json",
    "prepublishOnly_": "yarn run ncu && yarn run sort-package-json && yarn run test",
    "postpublish": "git commit -a -m \"chore(release): publish\"",
    "postpublish_": "git commit -m \"chore(release): publish\" .",
    "help": "yarn-tool --help",
    "ncu": "npx yarn-tool ncu -u",
    "sort-package-json": "npx yarn-tool sort"
  },
  "resolutions": {
    "@types/fs-extra": "*",
    "ansi-align": "*",
    "ansi-regex": "*",
    "ansi-styles": "*",
    "camelcase": "*",
    "camelcase-keys": "*",
    "colors": "*",
    "debug": "*",
    "indent-string": "*",
    "is-glob": "*",
    "is-npm": "*",
    "is-number": "*",
    "is-plain-object": "*",
    "meow": "*",
    "ms": "*",
    "package-json": "*",
    "pify": "*",
    "pkg-dir": "*",
    "pump": "*",
    "punycode": "*",
    "read-pkg": "*",
    "read-pkg-up": "*",
    "registry-url": "*",
    "require-main-filename": "*",
    "resolve": "*",
    "semver": "*",
    "set-value": "*",
    "slash": "*",
    "source-map": "*",
    "string-width": "*",
    "string_decoder": "*",
    "strip-ansi": "*",
    "strip-bom": "*",
    "strip-indent": "*",
    "supports-color": "*",
    "update-notifier": "*",
    "wrap-ansi": "*"
  },
  "dependencies": {
    "@ts-type/package-dts": "^1.0.8",
    "@yarn-tool/find-root": "^1.0.9",
    "@yarn-tool/static-file": "^1.0.27",
    "@yarn-tool/update-notifier": "^1.0.10",
