{
  "name": "yarn-tool",
  "version": "1.2.12",
  "description": "yarn cli tool helper",
  "keywords": [
    "cli",
    "data deduplication",
    "dedupe",
    "deduplicate",
    "deduplication",
    "diff",
    "duplicated",
    "install",
    "lerna",
    "lockfile",
    "npm",
    "package",
    "package manager",
    "packages",
    "project",
    "tool",
    "workspace",
    "workspaces",
    "ws",
    "yarn",
    "yarn-tool",
    "yarn.lock"
  ],
  "homepage": "https://github.com/bluelovers/yarn-tool#readme",
  "bugs": {
    "url": "https://github.com/bluelovers/yarn-tool/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluelovers/yarn-tool.git"
  },
  "license": "ISC",
  "author": "bluelovers",
  "main": "index.js",
  "bin": {
    "yarn-tool": "bin/yarn-tool.js",
    "yt": "bin/yt.js"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "install:resetLockfile": "ts-node ./bin/yarn-tool install --reset-lockfile",
    "npm:publish": "yarn run version:bump && npm publish",
    "version:bump": "ts-node ./bin/yarn-tool version",
    "prepublish:yarn:install": "ts-node ./bin/yarn-tool install",
    "prepublishOnly": "yarn run ncu && yarn run prepublish:yarn:install && yarn run sort-package-json",
    "prepublishOnly_": "yarn run prepublishOnly:update && yarn run prepublishOnly:check-bin && yarn run test",
    "prepublishOnly:check-bin": "ynpx --quiet @yarn-tool/check-pkg-bin",
    "prepublishOnly:lockfile": "ynpx --quiet sync-lockfile",
    "prepublishOnly:update": "yarn run ncu && yarn run sort-package-json",
    "postpublish": "yarn run postpublish:changelog && yarn run postpublish:git:commit && yarn run postpublish:git:tag && yarn run postpublish:git:push",
    "postpublish:changelog": "ynpx --quiet @yarn-tool/changelog && git add ./CHANGELOG.md",
    "postpublish:git:commit": "git commit -m \"chore(release): publish\" . & echo postpublish:git:commit",
    "postpublish:git:push": "git push --follow-tags",
    "postpublish:git:tag": "ynpx --quiet @yarn-tool/tag",
    "help": "ts-node ./bin/yarn-tool --help",
    "ncu": "ts-node ./bin/yarn-tool ncu -u",
    "sort-package-json": "ts-node ./bin/yarn-tool sort"
  },
  "dependencies": {
    "@ts-type/package-dts": "^1.0.51",
    "@yarn-tool/find-root": "^1.0.51",
    "@yarn-tool/ncu": "^1.0.77",
    "@yarn-tool/npm-package-arg-util": "^1.0.19",
    "@yarn-tool/pkg-deps-util": "^1.0.35",
    "@yarn-tool/semver-diff": "^1.0.26",
    "@yarn-tool/static-file": "^1.0.73",
    "@yarn-tool/table": "^1.0.34",
    "@yarn-tool/update-notifier": "^1.0.21",
    "@yarn-tool/version-recommended": "^1.0.34",
    "@yarn-tool/yarnlock": "^1.0.60",
    "@yarn-tool/yarnlock-ncu": "^1.0.26",
    "array-hyper-unique": "^1.1.10",
    "bluebird": "^3.7.2",
    "create-yarn-workspaces": "^1.0.120",
    "crlf-normalize": "^1.0.6",
    "cross-spawn-extra": "^2.1.0",
    "debug-color2": "^1.2.7",
    "find-yarn-workspace-root2": "^1.2.32",
    "fs-extra": "^10.0.0",
    "inquirer": "^8.1.4",
    "npm-init2": "^1.0.178",
    "os-locale": "<6 >=5",
    "package-json": "<7 >=6.5",
    "semver": "^7.3.5",
    "semver-utils": "^1.1.4",
    "sort-package-json": "^1.51.0",
    "sort-package-json3": "^1.0.39",
    "source-map-support": "^0.5.20",
    "string-natural-compare": "^3.0.1",
    "synp2": "^1.4.0",
    "terminal-link": "^3.0.0",
    "tslib": "^2.3.1",
    "upath2": "^3.1.10",
    "ws-pkg-list": "^1.0.65",
    "ws-root-spawn": "^1.0.24",
    "yargs": "^17.1.1",
    "yargs-parser": "^20.2.9",
    "yargs-unparser": "^2.0.0"
  },
  "devDependencies": {
    "@bluelovers/tsconfig": "^1.0.25",
    "@types/bluebird": "^3.5.36",
    "@types/deep-diff": "^1.0.1",
    "@types/fs-extra": "*",
    "@types/inquirer": "^8.1.1",
    "@types/lodash": "^4.14.172",
    "@types/node": "^16.9.1",
    "@types/semver": "^7.3.8",
    "@types/semver-utils": "^1.1.1",
    "@types/source-map-support": "^0.5.4",
    "@types/yargs": "^17.0.2",
    "@types/yargs-parser": "^20.2.1",
    "ts-type": "^1.2.40"
  },
  "peerDependencies": {
    "lerna": "^4.0.0",
    "yarn": "^1.22.11"
  }
}
